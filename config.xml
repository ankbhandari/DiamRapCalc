<?xml version='1.0' encoding='utf-8'?>
<widget id="com.AnkDroid.DiamondApp.Calc" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Diamond Rap Calculator</name>
    <description>
    AnkDroid Rap Calculator application that helps you to check for updated Rap Discount as per Updated Color or Clarity.
  </description>
    <author email="ank.bhandari@gmail.com" href="http://ankdroid.blogspot.com">
        Ankit Bhandari
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:+919930771310" />
    <allow-intent href="sms:+919930771310" />
    <allow-intent href="mailto:ank.bhandari@gmail.com" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="icon.png" />
    <platform name="android">
